{
  "name": "@dxfom/mtext",
  "version": "0.0.0",
  "description": "A low level DXF MTEXT content parser.",
  "private": true,
  "license": "GPL-3.0",
  "repository": "dxfom/mtext",
  "keywords": [
    "dxf"
  ],
  "main": "index.mjs",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc -p . && terser -c -m --toplevel -b beautify=false,semicolons=false --warn -o index.mjs index.js",
    "test": "npm run build && node index.spec.mjs"
  },
  "devDependencies": {
    "baretest": "^1.0.0",
    "terser": "^4.6.10",
    "typescript": "^3.8.3"
  }
}
